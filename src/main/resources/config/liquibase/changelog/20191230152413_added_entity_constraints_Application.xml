<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Application.
    -->
    <changeSet id="20191230152413-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="assignment_id"
                                 baseTableName="application"
                                 constraintName="fk_application_assignment_id"
                                 referencedColumnNames="id"
                                 referencedTableName="assignment"/>

        <addForeignKeyConstraint baseColumnNames="loanpool_id"
                                 baseTableName="application"
                                 constraintName="fk_application_loanpool_id"
                                 referencedColumnNames="id"
                                 referencedTableName="loanpool"/>

    </changeSet>
</databaseChangeLog>
